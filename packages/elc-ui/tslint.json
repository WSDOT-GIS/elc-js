{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": {
            "severity": "warn"
        },
        "object-literal-sort-keys": false,

        "prefer-const": {
            "severity": "warn"
        },
        "trailing-comma": {
            "options": {
                "multiline": "never",
                "singleline": "never"
            }
        }
    },
    "rulesDirectory": []
}