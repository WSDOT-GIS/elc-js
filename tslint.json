{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-plugin-prettier",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": true,
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "only-arrow-functions": false,
        "variable-name": {
            "options": ["allow-leading-underscore", "allow-pascal-case"]
        }
    },
    "rulesDirectory": []
}